pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                echo 'hi to all'
            }
      
         }
    
      
        stage('Test') {
            steps {
                echo 'Test World'
                echo 'hi to all'
            }
           
         }
          stage('Test1') {
            steps {
                echo 'testingd'
                echo 'hi to all'
            }
         }
         stage('Test2') {
            steps {
                echo 'testingd'
                echo 'hi to all'
            }
         }
      
    }
    
}

